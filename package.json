{
  "name": "env-struct",
  "version": "1.0.0",
  "description": "Schema-first environment variable loader with typed accessors and metadata.",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run format && tsup src/index.ts --dts --format esm,cjs --sourcemap --clean",
    "format": "prettier --write .",
    "lint": "tsc --project tsconfig.test.json",
    "test": "vitest run"
  },
  "keywords": [
    "env",
    "environment",
    "zod",
    "schema",
    "validation"
  ],
  "author": "Donobu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donobu-inc/env-struct.git"
  },
  "bugs": {
    "url": "https://github.com/donobu-inc/env-struct/issues"
  },
  "homepage": "https://github.com/donobu-inc/env-struct#readme",
  "peerDependencies": {
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "tsup": "^8.5.0",
    "typescript": "^5.4.0",
    "vitest": "^1.5.0"
  }
}